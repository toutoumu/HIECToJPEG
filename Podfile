target 'PickerDemo' do

  platform :ios, '8.0'
#use_frameworks!

# Recommended to keep GPUImage up-to-date
#pod 'GPUImage', :head
  pod 'GPUImage', "0.1.7"

# Optional for dynamic logging
  pod 'NBULog', "1.3.0"

# Optional for on-device log console
  pod 'LumberjackConsole', "3.3.0"

# 加密,这个是Object_C的最后一个版本,只能使用这个之前的版本 https://github.com/RNCryptor/RNCryptor-objc
  pod 'RNCryptor',"3.0.1"

# For development https://github.com/CyberAgent/iOS-NBUImagePicker
  pod 'NBUImagePicker', :path => 'DevelopmentPods/'

# 图片浏览器 https://github.com/mwaterfall/MWPhotoBrowser
  pod 'MWPhotoBrowser', :path => 'DevelopmentPods/'

# 图片裁剪 https://github.com/TimOliver/TOCropViewController
  pod 'TOCropViewController'


end

